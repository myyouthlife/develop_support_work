<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:esri="http://schemas.esri.com/arcgis/client/2009" x:Class="TestProjectofSL.identify_layers_set"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">

        <esri:Map Name="myMap" Background="White"  WrapAround="True" MouseClick="Map_MouseClick">
            <esri:ArcGISDynamicMapServiceLayer ID="mydynamiclayer" Url="http://192.168.220.120:6080/arcgis/rest/services/test_identify/MapServer/" VisibleLayers="0,1,2"/>
        </esri:Map>

    </Grid>
</UserControl>
