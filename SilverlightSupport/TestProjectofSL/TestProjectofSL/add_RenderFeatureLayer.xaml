<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:esri="http://schemas.esri.com/arcgis/client/2009" x:Class="TestProjectofSL.add_RenderFeatureLayer"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">


        <esri:Map Background="White" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" WrapAround="True" Height="290" Width="400">
            <esri:ArcGISDynamicMapServiceLayer Url="http://localhost:6080/arcgis/rest/services/MyMapServiceTest/MapServer"/>
        </esri:Map>


    </Grid>
</UserControl>
