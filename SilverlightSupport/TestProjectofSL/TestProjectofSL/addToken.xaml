    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">

        <esri:Map Name="myMap" Background="White" HorizontalAlignment="Left" VerticalAlignment="Top" WrapAround="True" Height="300" Width="400" Margin="-10,0,0,0">
           
